# Misc. Aliases
alias szr="source ~/.zshrc"
alias c="clear"
alias n="node"
alias ns="npm start"
alias ni="npm i"
alias nt="npm test"

# cd aliases
alias cdd="cd ~/Desktop"
alias cdo="cd ~/Documents/open-source"
alias cdm="cd ~/Documents/open-source/marko"
alias cdp="cd ~/Documents/projects"
